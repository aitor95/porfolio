{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@pages/*": [
        "src/components/pages/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
    }
  }
}